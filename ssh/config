Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa

# Should have my.github.com alias to /etc/resolve conf
Host my.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/my/id_rsa

Host *.lab.yyovkov.net
    User    labuser
    IdentityFile ~/.ssh/lab/id_rsa
